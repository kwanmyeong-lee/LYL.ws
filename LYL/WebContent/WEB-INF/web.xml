	</servlet>
	
	<servlet-mapping>
		<servlet-name>nowServ</servlet-name>
		<url-pattern>/NowServ2</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>insertServ</servlet-name>
		<servlet-class>com.mystudy.pd.serv.InsertServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>insertServ</servlet-name>
		<url-pattern>/InsertServ</url-pattern>
	</servlet-mapping>
	
